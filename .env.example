# Alpaca Trading Bot Configuration
# Copy this file to .env and fill in your actual credentials

# Alpaca API Credentials (get from https://app.alpaca.markets/develop/keys)
ALPACA_API_KEY=your_api_key_here
ALPACA_SECRET_KEY=your_secret_key_here

# Alpaca API Base URL
# For paper trading: https://paper-api.alpaca.markets
# For live trading: https://api.alpaca.markets
APCA_API_BASE_URL=https://paper-api.alpaca.markets

# Data source for market data (iex, sip, crypto_us)
DATA_SOURCE=iex
